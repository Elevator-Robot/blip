{
  "name": "blip",
  "version": "1.0.0",
  "description": "Build a simple static site",
  "main": "dist/index.js",
  "scripts": {
    "test": "jest",
    "build": "jsii",
    "package": "jsii-pacmak",
    "watch": "jsii -w"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aphexlog/blip.git"
  },
  "keywords": [
    "statc",
    "site"
  ],
  "author": {
    "name": "Aphexlog",
    "email": "aphexlog@gmail.com"
  },
  "contributors": [
    {
      "name": "Aphexlog",
      "email": "aphexlog@gmail.com"
    },
    {
      "name": "John Weland",
      "email": "john.weland@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aphexlog/blip/issues"
  },
  "types": "dist/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "java": {
        "package": "com.aphexlog.blip",
        "maven": {
          "groupId": "com.aphexlog",
          "artifactId": "blip"
        }
      },
      "dotnet": {
        "namespace": "Aphexlog.Blip",
        "packageId": "Aphexlog"
      }
    }
  },
  "homepage": "https://github.com/aphexlog/blip#readme",
  "devDependencies": {
    "jsii": "^1.72.0",
    "jsii-pacmak": "^1.72.0"
  }
}
