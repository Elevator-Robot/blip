{
  "name": "@elevator-robot/blip",
  "version": "1.0.20",
  "description": "Build a simple static site",
  "main": "src/index.js",
  "scripts": {
    "lint": "eslint .",
    "test": "tsc --build tsconfig.json && jest --runInBand",
    "build": "jsii",
    "package": "jsii-pacmak",
    "watch": "jsii -w",
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elevator-robot/blip.git"
  },
  "keywords": [
    "statc",
    "site"
  ],
  "author": {
    "name": "Aphexlog",
    "email": "aphexlog@gmail.com"
  },
  "contributors": [
    {
      "name": "Aphexlog",
      "email": "aphexlog@gmail.com"
    },
    {
      "name": "John Weland",
      "email": "john.weland@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/elevator-robot/blip/issues"
  },
  "types": "src/index.d.ts",
  "jsii": {
    "outdir": "dist",
    "targets": {
      "python": {
        "distName": "blip",
        "module": "blip",
        "project_urls": {
          "source": "https://github.com/elevator-robot/blip/",
          "issues": "https://github.com/elevator-robot/blip/issues"
      }
    }
  },
  "homepage": "https://github.com/elevator-robot/blip#readme"
},
"devDependencies": {
  "@types/node": "^18.11.18",
  "aws-cdk-lib": "^2.58.1",
  "jsii": "^1.72.0",
  "jsii-pacmak": "^1.72.0",
  "constructs": "10.1.203"
},
"peerDependencies": {
  "constructs": "10.1.203"
},
"dependencies": {
  "aws-cdk-lib": "^2.58.1"
}
}
