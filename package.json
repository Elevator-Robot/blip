{
  "name": "@elevator-robot/blip",
  "version": "1.0.7",
  "description": "Build a simple static site",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "test": "jest",
    "build": "jsii",
    "package": "jsii-pacmak",
    "watch": "jsii -w",
    "compile": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aphexlog/blip.git"
  },
  "keywords": [
    "statc",
    "site"
  ],
  "author": {
    "name": "Aphexlog",
    "email": "aphexlog@gmail.com"
  },
  "contributors": [
    {
      "name": "Aphexlog",
      "email": "aphexlog@gmail.com"
    },
    {
      "name": "John Weland",
      "email": "john.weland@gmail.com"
    }
  ],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/aphexlog/blip/issues"
  },
  "types": "src/index.d.ts",
  "jsii": {
    "outdir": "./dist",
    "targets": {
      "java": {
        "package": "com.aphexlog.blip",
        "maven": {
          "groupId": "com.aphexlog",
          "artifactId": "blip"
        }
      },
      "python": {
        "distName": "dist",
        "module": "blip"
      },
      "dotnet": {
        "namespace": "Aphexlog.Blip",
        "packageId": "Aphexlog"
      }
    }
  },
  "homepage": "https://github.com/aphexlog/blip#readme",
  "devDependencies": {
    "@types/node": "^18.11.18",
    "aws-cdk-lib": "^2.58.0",
    "jsii": "^1.72.0",
    "jsii-pacmak": "^1.72.0",
    "constructs": "10.1.203"
  },
  "peerDependencies": {
    "constructs": "10.1.203"
  },
  "dependencies": {}
}
